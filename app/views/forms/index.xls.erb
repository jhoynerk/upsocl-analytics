
<table border="1">
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
  <% @forms.each do |formulario| %>
  <tr>
    <td><%= formulario.id %></td>
    <td><%= formulario.name %></td>
    <td><%= formulario.last_name %></td>
    <td><%= formulario.email %></td>
  </tr>
  <% end %>
</table>
