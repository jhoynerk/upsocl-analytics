ul
  - urls.each do |u|
    li
      p
        strong Url Id
        span = " = #{u.id}"
      p
        strong Url
        span = " = #{u.data}"
      p
        strong Line ID
        span = " = #{u.line_id}"
      p
        strong Pais
        span = " = #{u.countries.map(&:name).to_s} "
      p
        strong Tags
        span = " = #{u.tags.map(&:title).to_s} "
      p
        strong Account Analytics
        span = " = #{u.profile_id}"
      p
        strong Frecuencia de actualizaci√≥n
        span = " = #{u.interval_status_humanize}"
      p
        strong Facebook Post Count
        span = " = #{u.facebook_posts.count}"
        ul
          - u.facebook_posts.each do |fb|
            li
              p
                strong User facebook
                - if fb.facebook_account.nil?
                  span No selecciono el facebook.
                - else
                  span = " = #{fb.facebook_account.name}  "
                strong Post ID facebook
                span = " = #{fb.post_id}"
